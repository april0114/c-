#include <stdio.h>
void pr_str_array(char** dp);
int main(void)
{
	char *q;
	pr_str_array(&q);
}
void pr_str_array(char** dp)
{
	 char *ap[] = {
	   " A bad shearer never had a good sickle",
	   " A bad workman (always) blames his tools",
	   " A bad workman quarrels with his tools",
	   " A bad thing never dies",
	   " A bad workman finds fault with his tools"
	};
	 for (int n = 0; n < 5; n++)
		 printf("%s\n", ap[n]);
}
