#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>
int main(void)
{
	char c[50] = "meet me at midnight";
	char s[50] = "____ __ __ ________";
	int i;
	char ch;

	while (1)
	{
		printf("글자를 입력하세요 : %s", s);
		ch = getch(); 
		for (i = 0; i != NULL; i++)
		{
			if (ch == s[i])
				c[i] = ch;
		}
		if (strcmp(c, s) == 0)
			break;
	}
	return 0;
}
